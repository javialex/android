<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:columnCount="2"
    android:rowCount="2">
    <TextView
        android:id="@+id/txvEventoMovil"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Evento Movil"/>

    <TextView
        android:id="@+id/txvLatitud"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Latitud"/>
    <TextView
        android:id="@+id/txvLongitud"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Longitud"/>
    <TextView
        android:id="@+id/txvRadio"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Radio"/>
    <TextView
        android:id="@+id/txvFechaIni"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Fecha Ini"/>
    <TextView
        android:id="@+id/txvFechaFin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Fecha Fin"/>
    <TextView
        android:id="@+id/txvCantMarcacionesDia"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Cantidad"/>
    <TextView
        android:id="@+id/txvObservacion"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Obs"/>
</GridLayout>